{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "cryptoUri": {
      "type": "string"
    },
    "emailInvoiceId": {
      "type": "string"
    },
    "buttonInvoiceId": {
      "type": "string"
    },
    "flags": {
      "type": "object",
      "properties": {
        "refundable": {
          "type": "boolean"
        },
        "notRefundableCause": {
          "enum": [
            "REFUND_AMOUNT_ZERO",
            "REFUND_AMOUNT_TOO_LOW",
            "WRONG_INVOICE_STATE",
            "NOT_ALLOWED"
          ]
        },
        "resolvableStatus": {
          "enum": [
            "YES",
            "WAITING",
            "NO"
          ]
        }
      }
    },
    "merchantAmount": {
      "type": "object",
      "properties": {
        "currency": {
          "type": "string"
        },
        "amount": {
          "type": "string"
        }
      }
    },
    "settlementAmount": {
      "type": "object",
      "properties": {
        "currency": {
          "type": "string"
        },
        "amount": {
          "type": "string"
        }
      }
    },
    "notifyEmail": {
      "type": "string"
    },
    "notifyUrl": {
      "type": "string"
    },
    "paid": {
      "type": "object",
      "properties": {
        "amount": {
          "type": "string"
        },
        "diff": {
          "type": "string"
        },
        "currency": {
          "type": "string"
        }
      }
    },
    "paidSince": {
      "type": "number"
    },
    "product": {
      "type": "object",
      "properties": {
        "description": {
          "type": "string"
        },
        "name": {
          "type": "string"
        }
      }
    },
    "rate": {
      "type": "object",
      "properties": {
        "currencyFrom": {
          "type": "string"
        },
        "currencyTo": {
          "type": "string"
        },
        "value": {
          "type": "string"
        }
      }
    },
    "reference": {
      "type": "string"
    },
    "refunds": {
      "type": "array",
      "items": {
        "type": "object"
      }
    },
    "settlement": {
      "type": "object",
      "properties": {
        "amount": {
          "type": "string"
        },
        "createdAt": {
          "type": "number"
        },
        "currency": {
          "type": "string"
        },
        "fee": {
          "type": "string"
        },
        "id": {
          "type": "string"
        }
      }
    },
    "requiredConfirmations": {
      "type": "number"
    },
    "requiredConfirmationsToRefund": {
      "type": "number"
    },
    "returnUrl": {
      "type": "string"
    },
    "status": {
      "enum": [
        "active",
        "confirming",
        "paid",
        "expired",
        "error"
      ]
    },
    "takeInfo": {
      "type": "object",
      "properties": {
        "taken": {
          "type": "boolean"
        },
        "takenAt": {
          "type": "number"
        },
        "expiration": {
          "type": "number"
        },
        "expirationDuration": {
          "type": "number"
        },
        "originalSettlementAmount": {
          "type": "number"
        },
        "offeredPrice": {
          "type": "number"
        },
        "offeredSettlementAmount": {
          "type": "number"
        },
        "customerCurrency": {
          "type": "string"
        },
        "settlementCurrency": {
          "type": "string"
        },
        "refundAmount null": {
          "type": "number"
        }
      }
    },
    "timeoutTime": {
      "type": "number"
    },
    "cryptoTransactions": {
      "type": "array",
      "items": {
        "type": "object"
      }
    },
    "unhandledExceptions": {
      "type": "boolean"
    },
    "url": {
      "type": "string"
    },
    "refundLink": {
      "type": "string"
    },
    "id": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "confirmations": {
      "type": "number"
    },
    "confirmingSince": {
      "type": "number"
    },
    "createdAt": {
      "type": "number"
    },
    "customerAmount": {
      "type": "object",
      "properties": {
        "currency": {
          "type": "string"
        },
        "amount": {
          "type": "string"
        }
      }
    },
    "expiredSince": {
      "type": "number"
    },
    "errorSince": {
      "type": "number"
    },
    "activeSince": {
      "type": "number"
    },
    "preparedSince": {
      "type": "number"
    }
  }
}